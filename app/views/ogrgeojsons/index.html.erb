<p id="notice"><%= notice %></p>

<h1>Listing Regions</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ogrgeojsons.each do |ogrgeojson| %>
      <tr>
        <td><%= ogrgeojson.id %></td>
        <td><%= ogrgeojson.name %></td>
        <td><%= link_to 'Show', ogrgeojson %></td>
        <td><%= link_to 'Edit', edit_ogrgeojson_path(ogrgeojson) %></td>
        <td><%= link_to 'Destroy', ogrgeojson, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ogrgeojson', new_ogrgeojson_path %>
